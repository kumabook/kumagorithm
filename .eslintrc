{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-multi-spaces": ["error", {
      exceptions: {
        "VariableDeclarator": true,
        "ImportDeclaration": true,
        "Property": false
      }
    }],
    "react/prefer-stateless-function": 0,
    "react/prefer-es6-class": 0
  }
}
